[fcgi-program:smokeping]]
command=/srv/http/smokeping/smokeping.fcgi
socket=unix:///run/%(program_name)s.sock
socket_owner=http
socket_mode=0777
process_name=%(program_name)s_%(process_num)02d
numprocs=4
directory=/tmp
umask=022
priority=999
autostart=true
autorestart=unexpected
startsecs=1
startretries=3
exitcodes=0
stopsignal=QUIT
stopasgroup=false
killasgroup=false
stopwaitsecs=10
user=smokeping
redirect_stderr=true
stdout_logfile=/dev/stdout
stderr_logfile=/dev/stderr
serverurl=AUTO

